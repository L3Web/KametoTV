{% extends "base.html.twig" %}
{% block title %}{% trans %}Error 404{% endtrans %}{% endblock %}
{% block body %}
    <div class="mt-3 ml-3">
        <h1>{% trans %}Page not found{% endtrans %}</h1>
        <p>{% trans %}
                The requested page couldn't be located. Checkout for any URL
                misspelling or{% endtrans %} <a
                    href="{{ path('app_base_home') }}">{% trans %}return to the homepage{% endtrans %}</a>.
        </p>
    </div>
{% endblock %}